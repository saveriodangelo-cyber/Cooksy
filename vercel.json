{
    "buildCommand": "echo 'Static files'",
    "outputDirectory": "ui",
    "framework": "static",
    "env": {
        "API_BASE_URL": "https://cooksy-finaly.up.railway.app"
    },
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}