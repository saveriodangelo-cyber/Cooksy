{
    "version": 2,
    "name": "cooksy-app",
    "builds": [
        {
            "src": "ui/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/ui/$1"
        }
    ],
    "env": {
        "API_BASE_URL": "https://cooksy-finaly.up.railway.app"
    }
}