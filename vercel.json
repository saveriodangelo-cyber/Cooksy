{
    "buildCommand": "echo 'Building Cooksy PWA'",
    "outputDirectory": "ui",
    "env": {
        "API_BASE_URL": "@api_base_url"
    },
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}